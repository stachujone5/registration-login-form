{
  "root": true,
  "parserOptions": {
    "project": "tsconfig.json"
  },
  "plugins": ["@typeofweb"],
  "extends": ["next/core-web-vitals", "plugin:@typeofweb/recommended"],
  "rules": {
    "@typeofweb/capitalize-modules": "off",
    "import/no-default-export": 0,
    "react/self-closing-comp": [
      "error",
      {
        "component": true,
        "html": true
      }
    ]
  }
}
